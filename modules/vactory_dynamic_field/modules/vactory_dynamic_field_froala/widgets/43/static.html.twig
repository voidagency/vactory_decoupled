<div class="fdb-block fdb-block-hasbackground bg-dark">
    <div class="row justify-content-center text-center">
        <div class="col">

            {% for item in 0..5 %}
                {% set placehold = "https://picsum.photos/id/" ~ random(1, 200) ~ "/80/50.jpg" %}
                <img alt="Image Alt" height="40"
                    class="ml-3 mr-3 mb-2 mt-2 lazyload"
                    src="{{ content.image_placeholder }}"
                    data-src="{{ placehold }}">
            {% endfor %}
        </div>
    </div>
</div>
