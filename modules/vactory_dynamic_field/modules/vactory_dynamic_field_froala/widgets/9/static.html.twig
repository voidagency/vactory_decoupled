<div class="fdb-block">
    <div class="col text-center">
            <h2>Title</h2>
    </div>
    <div class="row pt-4 vf-slick-slider vf-slick-mobile" data-arrows="false">
        {% for i in 0..3 %}
                {% set placehold = "https://picsum.photos/id/" ~ random(1, 200) ~ "/255/155.jpg" %}
                 <div class="col-12 col-sm-6 col-lg-3 d-flex justify-content-center mb-4 mb-lg-0 ">
                 <img alt="Image Alt"
                    class="img-fluid lazyload mb-4"
                    src="{{ content.image_placeholder }}"
                    data-src="{{ placehold }}">
                 </div>
        {% endfor %}
    </div>
</div>
